{
  "compilerOptions": {
    "target": "es6",
    "outDir": "scripts/",
    "noImplicitAny": false,
    "sourceMap": true,
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules"
  ]
}
